{
  "manifest_version": 2,

  "name": "vk-x",
  "description": "Extension for vk.com that makes life easier",
  "version": "1.3.0",

  "icons": {
    "48": "icons/vk-x-48.png",
    "128": "icons/vk-x-128.png"
  },

  "content_scripts": [{
    "run_at": "document_start",
    "matches": ["https://new.vk.com/*"],
    "css": ["styles.css"]
  }, {

    "run_at": "document_end",
    "matches": ["https://new.vk.com/*"],
    "js": ["content-script.js"]
  }]
}
